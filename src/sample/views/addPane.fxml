            <MenuItem fx:id="underMenu" mnemonicParsing="false" onAction="#underAction" text="Underground" />
            <MenuItem fx:id="castleMenu" mnemonicParsing="false" onAction="#castleAction" text="Castle" />
            <MenuItem fx:id="houseMenu" mnemonicParsing="false" onAction="#houseAction" text="House" />
         </items>
      </MenuButton>
      <Label layoutX="269.0" layoutY="114.0" prefWidth="61.0" text="Description" />
      <TextArea fx:id="discriptionText" layoutX="38.0" layoutY="138.0" prefHeight="173.0" prefWidth="524.0" text="Text" wrapText="true" />
      <Label layoutX="14.0" layoutY="42.0" text="Map Name" />
      <TextField fx:id="mapNameText" layoutX="73.0" layoutY="38.0" prefHeight="25.0" prefWidth="149.0" text="Map" />
