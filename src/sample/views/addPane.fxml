            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="233.0" layoutY="42.0" text="Height:" />
      <TextField fx:id="heightSize" layoutX="272.0" layoutY="38.0" prefHeight="25.0" prefWidth="31.0" text="1" />
      <Label layoutX="313.0" layoutY="42.0" text="Width:" />
      <TextField fx:id="widthSize" layoutX="348.0" layoutY="38.0" prefHeight="25.0" prefWidth="31.0" text="1" />
      <Label layoutX="396.0" layoutY="42.0" text="Scene" />
