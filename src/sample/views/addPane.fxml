<AnchorPane focusTraversable="true" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.AddPaneController">
   <children>
      <Button fx:id="saveButton" layoutX="381.0" layoutY="327.0" mnemonicParsing="false" onMouseClicked="#saveAction" prefHeight="39.0" prefWidth="93.0" text="Save">
